{
  "name": "@devix-tecnologia/directus2ts",
  "version": "1.0.4",
  "description": "Create typescript types from any directus api",
  "main": "dist/index.js",
  "bin": {
    "directus2ts": "./dist/cli.js"
  },
  "type": "module",
  "types": "./dist/types/*.d.ts",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/devix-tecnologia/directus2ts"
  },
  "scripts": {
    "test-cli": "tsx src/cli.ts",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && yarn build:tsc && chmod u+x ./dist/cli.js",
    "build:tsc": "tsc -p ./tsconfig.json",
    "prepack": "yarn build",
    "semantic-release": "semantic-release"
  },
  "author": "Roberto Seba@Devix 2023",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^22.10.2",
    "@types/node-fetch": "^2.5.10",
    "@types/typescript": "^2.0.0",
    "@types/yargs": "^17.0.0",
    "semantic-release": "^24.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "change-case": "^5.4.4",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^15.0.3",
    "node-fetch": "^3.3.2",
    "openapi-typescript": "^7.4.4",
    "yargs": "^17.6.2",
    "zod": "^3.20.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
